{
  "name": "find-a-friend",
  "version": "1.0.0",
  "description": "RESTful project using Node, TS and Fastify",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "test:create": "npm link ./prisma/vitest-environment-prisma",
    "test:install": "npm link vitest-environment-prisma",
    "test": "vitest run --config ./vite.config.ts --reporter verbose",
    "pretest:e2e": "run-s test:create test:install",
    "test:e2e": "vitest run --config ./vite.e2e.config.ts --reporter verbose",
    "dev": "tsx watch src/server.ts",
    "prd": "node build/server.js",
    "build": "tsup --out-dir build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "1.3.95",
    "@types/bcryptjs": "2.4.5",
    "@types/node": "20.8.10",
    "@types/supertest": "2.0.15",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.5.2",
    "supertest": "6.3.3",
    "tsup": "7.2.0",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "unplugin-swc": "1.4.3",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@faker-js/faker": "8.2.0",
    "@fastify/cookie": "9.1.0",
    "@fastify/jwt": "7.2.2",
    "@fastify/swagger": "8.12.0",
    "@fastify/swagger-ui": "1.10.1",
    "@prisma/client": "5.5.2",
    "bcryptjs": "2.4.3",
    "dotenv": "16.3.1",
    "fastify": "4.24.3",
    "zod": "3.22.4"
  }
}
